{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server=YourServerName;Database=YourDatabaseName;Trusted_Connection=True;TrustServerCertificate=True;MultipleActiveResultSets=true"
  },
  "JwtSettings": {
    "Secret": "YourSecretHere",
    "Issuer": "Body4uHUB.Identity",
    "Audience": "Body4uHUB",
    "ExpirationMinutes": 60
  },
  "MassTransit": {
    "Host": "YourHost",
    "VirtualHost": "/",
    "Username": "YourUsername",
    "Password": "YourPassword",
    "RetryCount": 5,
    "RetryIntervalSeconds": 5,
    "OutboxQueryMessageLimit": 100,
    "OutboxQueryDelaySeconds": 1
  }
}
